    <div>
        <div class="content-wrapper">

            <div class="page-content-floater">
                <!--<section class="page-content-wrapper">-->

                <div class="page-content">
                    <!--<div class="row top-sp-md">-->
                    <!--<div class="col-md-10 middle">-->

                    <div class="panel panel-login">
                        <img class="login-logo" src="assets/images/logo_2.png">
                        <!-- <div class="panel-heading">Login</div> -->
                        <div class="panel-body">
                            <div class="row">
                                <div class="">
                                    <span class="form-group">
                                        <span style="color: #fcd116">
                                            Username
                                        </span>
                                        <span>
                                            {{input type="text"  placeholder="Username" value=uname id="txtUsername" enter="clickLoginBtn" class="form-control"}}
                                        </span>
                                    </span>
                                    <span class="form-group">
                                        <span style="color: #fcd116">
                                            Email
                                        </span>
                                        <span>
                                            {{input type="text"  placeholder="Email" value=email id="email" enter="clickLoginBtn" class="form-control"}}
                                        </span>
                                    </span>
                                    <div class="form-group">
                                        <span style="color: #fcd116">
                                            Phone
                                        </span>
                                        <span>
                                            {{input type="text" placeholder="phone" value=phone id="txtphone" enter="clickLoginBtn" class="form-control"}}
                                        </span>
                                    </div>
                                    <div class="form-group">
                                        <span style="color: #fcd116">
                                            Password
                                        </span>
                                        <span>
                                            {{input type="password" placeholder="Password" value=pwd id="txtPassword" enter="clickLoginBtn" class="form-control"}}
                                        </span>
                                    </div>

                                    <div class="form-group">
                                        <span style="color: #fcd116">
                                            Confirm Password
                                        </span>
                                        <span>
                                            {{input type="password" placeholder="confirmPassword" value=confpwd id="confirmPassword" enter="clickLoginBtn" class="form-control"}}
                                        </span>
                                    </div>
                                    <div class="login-error-msg error-message" id="loginMsg">
                                        <!--<span style="color: white"></span>-->
                                    </div>
                                    <div class="login">
                                        <button class="btn-login" {{action "clickSignUp" this}}> Sign Up
                                        </button>
                                    </div>

                                    <div class="login">
                                        <button class="btn-login"  {{action "setSignUp"}}>Back</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!--</div>-->
                    <!--</div>-->
                </div>
                <div class="footer">

                </div>
                <!--</section>-->
            </div>
        </div>

    </div>